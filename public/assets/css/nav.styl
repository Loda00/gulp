nav 
    width: 100%
    height: 80px
    background-color: rgb(9,62,94)
    display: flex

.nav-1
    position relative
    width: 50%
    height: 100%
    display: flex
    align-items: center
    img
        // display: block
        // margin: auto
        margin-left: 12%

.nav-2
    width: 50%
    height: 100%
    display: flex
    align-items: center
    justify-content: space-around
    padding-right: 10%
    span
        font-family: 'Courier New', Courier, monospace
        font-size: 12px
        cursor pointer
        &:hover
            color: white
            transition: .5s ease
        a
            text-decoration: none 
            color: #89B1CC